	public int getXforCenteredText(String text) {
		int length =(int)g2.getFontMetrics().getStringBounds(text, g2).getWidth();
		int x=gs.screenWidth/2 - length/2;
		return x;
	}
	
	public int getXforAlignToRightText(String text,int tailX) {
		int length =(int)g2.getFontMetrics().getStringBounds(text, g2).getWidth();
		int x=tailX - length;
		return x;
	}
